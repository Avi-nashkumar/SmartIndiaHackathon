<html>
    <head>
        <title><% title %></title>
        <meta name="viewport" content="width=device-width, initial-scale=1">
        <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.1/css/bootstrap.min.css">
        <link rel="stylesheet" href="https://www.w3schools.com/w3css/4/w3.css">
        <link rel="stylesheet" href="css/pages_back_grad.css">
    </head>

    <body>
        <div class="w3-container">
            <%= records.forEach(function(userdata){%)>
            <h2>User ID : <span id="user_id"><%= userdata.userid %></span></h2>
            <br><br>
            <div class="w3-card-4" style="width:100%;">
              <header class="w3-container w3-light-grey">
                <h3>Name of the Applicant : <span id="name"> <%= userdata.name %></span></h3>
              </header>
              <div class="w3-container">
                <p>Full Address : <span id="address"><%= userdata.address %></span></p>
                <hr>
                <% if(userdata.image){ %>
                <img src="./images/<% userdata.image %>" alt="Applicant" class="w3-left w3-circle w3-margin-right" style="width:250px">
                <% } %>
                <p>Email Address : <span id="email"><%= userdata.email %></span></p>
                <br>
                <p>Contact Number : <span id="contact_num"><%= userdata.contact %></span></p>
                <br>
                <p>Batch Number : <span id="batch_num"><%= userdata.batch %></span></p>
                <br>
                <p>Nationality : <span id="Nationality"><%= userdata.nationality %></span></p>
                <br>
                <hr>
              </div>
              <button class="w3-button w3-block w3-dark-grey">Update Settings</button>
            </div>
            <% }) %>
          </div>
            <br><br>
            <hr>
            <input type="button" class="btn btn-success" value="Go Back" onclick="history.back(-1)" />
    </body>
</html>